/**
 * @file
 * Defines default strategy parameter values for the given timeframe.
 */

// Defines indicator's parameter values for the given pair symbol and timeframe.
struct Indi_AD_Params_{{ Timeframe }} : ADParams {
  Indi_AD_Params_{{ Timeframe }}() : ADParams(indi_ad_defaults, PERIOD_{{ Timeframe }}) { shift = {{ AD_Indi_AD_Shift | default(0) }}; }
} indi_ad_{{ Timeframe | lower }};

// Defines strategy's parameter values for the given pair symbol and timeframe.
struct Stg_AD_Params_{{ Timeframe }} : StgParams {
  // Struct constructor.
  Stg_AD_Params_{{ Timeframe }}() : StgParams(stg_ad_defaults) {
    lot_size = {{ AD_LotSize | default(0) }};
    signal_open_method = {{ AD_SignalOpenMethod | default(0) }};
    signal_open_filter_method = {{ AD_SignalOpenFilterMethod | default(1) }};
    signal_open_level = (float){{ AD_SignalOpenLevel | default(0) }};
    signal_open_boost = {{ AD_SignalOpenBoostMethod | default(0) }};
    signal_close_method = {{ AD_SignalCloseMethod | default(0) }};
    signal_close_level = (float){{ AD_SignalCloseLevel | default(0) }};
    price_stop_method = {{ AD_PriceStopMethod | default(0) }};
    price_stop_level = (float){{ AD_SignalCloseLevel | default(1) }};
    tick_filter_method = {{ AD_TickFilterMethod  | default(1) }};
    max_spread = {{ AD_MaxSpread | default(0) }};
  }
} stg_ad_{{ Timeframe | lower }};
